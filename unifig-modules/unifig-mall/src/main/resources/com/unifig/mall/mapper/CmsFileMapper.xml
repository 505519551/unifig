<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.unifig.mall.mapper.CmsFileMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.unifig.mall.bean.model.CmsFile">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="suffix" property="suffix" />
        <result column="url" property="url" />
        <result column="absolute_path" property="absolutePath" />
        <result column="upload_time" property="uploadTime" />
        <result column="create_time" property="createTime" />
        <result column="edit_time" property="editTime" />
        <result column="ratel_no" property="ratelNo" />
        <result column="enable" property="enable" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, suffix, url, absolute_path, upload_time, create_time, edit_time, ratel_no, enable
    </sql>

</mapper>
